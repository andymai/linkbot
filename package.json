{
  "name": "linkbot",
  "version": "1.0.0",
  "description": "a Slack bot that lets you save and retrieve text snippets",
  "keywords": [
    "Slack",
    "bot",
    "bookmark"
  ],
  "main": "lib/linkbot.js",
  "bin": {
    "linkbot": "bin/bot.js"
  },
  "scripts": {
    "start": "node bin/bot.js",
    "test": "xo bin/bot.js lib/linkbot.js"
  },
  "author": "Andy Mai <hi@andymai.com>",
  "license": "MIT",
  "engines": {
    "node": ">=0.10.0"
  },
  "dependencies": {
    "async": "^2.0.0-rc.3",
    "request": "^2.60.0",
    "slackbots": "^0.3.0",
    "sqlite3": "^3.0.10",
    "q": "^1.4.1",
    "weather-js": "^2.0.0"
  },
  "devDependencies": {
    "xo": "^0.5.2"
  },
  "xo": {
    "space": 4,
    "rules": {
      "camelcase": [
        2,
        {
          "properties": "never"
        }
      ]
    }
  }
}
